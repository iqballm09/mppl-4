<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/foodcourtapp_white"
        android:label="@string/app_name"
        android:roundIcon="@drawable/foodcourtapp_white"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodCourtAppMerchant"
        tools:targetApi="31">
        <activity
            android:name=".layout.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.TransactionHistoryActivity"
            android:parentActivityName=".layout.MainActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.StartActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.LoginActivity"
            android:exported="false"
            android:parentActivityName=".layout.StartActivity">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.DetailTransactionActivity"
            android:exported="false"
            android:parentActivityName=".layout.TransactionHistoryActivity">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.WithdrawHistoryActivity"
            android:parentActivityName=".layout.WithdrawActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.ConfirmationActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.EnterPinActivity"
            android:exported="false"
            android:parentActivityName=".layout.WithdrawReceiptActivity">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.WithdrawReceiptActivity"
            android:exported="false"
            android:parentActivityName=".layout.WithdrawActivity">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.WithdrawActivity"
            android:exported="false"
            android:parentActivityName=".layout.WithdrawMenuActivity">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.WithdrawMenuActivity"
            android:exported="false"
            android:parentActivityName=".layout.MainActivity">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.NotificationActivity"
            android:exported="false"
            android:parentActivityName=".layout.MainActivity">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".layout.MainActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
    </application>
</manifest>